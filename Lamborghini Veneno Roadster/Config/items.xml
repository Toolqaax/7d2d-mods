<configs>
	<append xpath="/items">
		<item name="Lamborghini Veneno Body">
			<property name="Extends" value="vehicleBicycleHandlebars"/>
			<property name="CustomIcon" value="venenobody" />
			<property name="SellableToTrader" value="True"/>
			<property name="DescriptionKey" value="Apollo Body"/>
			<property name="EconomicValue" value="9000"/>
			<property name="UnlockedBy" value="craftingVehicles"/>
		</item>

        <item name="vehicleLamborghiniVenenoplaceable">
			<property name="Extends" value="vehicleMinibikePlaceable"/>
			<property name="CustomIcon" value="venenoicon" />
			<property name="Tags" value="vehicle,vfuel,vlight"/>
			<property name="Meshfile" value="#@modfolder:Resources/veneno.unity3d?LamboVeneno"/>
			<property name="EconomicValue" value="35000"/>
			<property class="Action1">
				<property name="Vehicle" value="vehicleLamborghiniVeneno"/>
				<property name="VehicleSize" value="4.7, 4.8, 5"/>
			</property>
			<effect_group name="Base Effects">
				<passive_effect name="ModSlots" operation="base_set" value="4"/>
				<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
				<passive_effect name="DegradationMax" operation="base_set" value="10000"/>
			</effect_group>
		</item>
		


	</append>
</configs>